{
  "version": "2.0.0",
  "scripts": {
    "start": "webpack serve --progress --watch --content-base build --port 8000",
    "build": "NODE_ENV=production webpack",
    "build:production": "npm run clean && npm run build && npx cap sync",
    "build:resources": "mkdir -p resources && cp -R other/designs/android resources && cp -R other/designs/splash.png resources && npx sharp -i other/designs/icon.svg -o resources/icon.png resize 1024 1024 -- removeAlpha && npx cordova-res ios --skip-config --resources resources --copy && npx cordova-res android --skip-config --resources resources --copy",
    "deploy": "vercel --prod -A other/now.json -t $VERCEL_TOKEN --confirm",
    "clean": "rm -rf build resources",
    "test": "echo 'No unit tests yet'",
    "test:eslint": "eslint src --quiet --ext .jsx --ext .js && echo '\\033[37;42;1m PASS \\033[00m'"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run test:eslint"
    }
  },
  "dependencies": {
    "@bit/flumens.apps.attr-page": "2.0.1",
    "@bit/flumens.apps.auto-suggest-input": "1.0.3",
    "@bit/flumens.apps.date": "0.2.1",
    "@bit/flumens.apps.device": "0.2.1",
    "@bit/flumens.apps.header": "0.6.3",
    "@bit/flumens.apps.helpers.alert": "0.4.2",
    "@bit/flumens.apps.helpers.loader": "0.1.1",
    "@bit/flumens.apps.helpers.toast": "0.2.0",
    "@bit/flumens.apps.info-background-message": "0.1.2",
    "@bit/flumens.apps.info-message": "^1.2.1",
    "@bit/flumens.apps.input-with-validation": "^0.2.1",
    "@bit/flumens.apps.main": "0.2.0",
    "@bit/flumens.apps.menu-attr-item": "1.1.2",
    "@bit/flumens.apps.menu-attr-item-from-model": "1.1.1",
    "@bit/flumens.apps.menu-note-item": "^0.2.2",
    "@bit/flumens.apps.modal-header": "0.1.4",
    "@bit/flumens.apps.model-location-map": "2.0.3",
    "@bit/flumens.apps.models.init-stored-samples": "0.1.0",
    "@bit/flumens.apps.models.media": "0.2.4",
    "@bit/flumens.apps.models.media-with-drupal-remote": "0.0.1",
    "@bit/flumens.apps.models.model": "0.1.0",
    "@bit/flumens.apps.models.occurrence": "0.4.0",
    "@bit/flumens.apps.models.occurrence-with-drupal-remote": "1.0.0",
    "@bit/flumens.apps.models.sample": "0.4.0",
    "@bit/flumens.apps.models.sample-with-drupal-remote": "1.0.0",
    "@bit/flumens.apps.models.store": "0.3.1",
    "@bit/flumens.apps.page": "1.1.0",
    "@bit/flumens.apps.photo-picker": "1.1.0",
    "@bit/flumens.apps.route-with-models": "0.3.1",
    "@bit/flumens.apps.section": "0.4.0",
    "@bit/flumens.apps.toggle": "^0.1.0",
    "@bit/flumens.apps.utils.analytics": "0.1.0",
    "@bit/flumens.apps.utils.location": "0.1.5",
    "@bit/flumens.apps.utils.validation": "0.1.2",
    "@capacitor/android": "2.4.7",
    "@capacitor/core": "2.4.7",
    "@capacitor/ios": "2.4.7",
    "@indicia-js/core": "5.4.0",
    "@indicia-js/remote-drupal": "5.6.7",
    "@ionic-native/core": "5.31.1",
    "@ionic/core": "5.6.0",
    "@ionic/react": "5.6.0",
    "@ionic/react-router": "5.6.0",
    "@sentry/browser": "6.2.1",
    "axios": "0.21.1",
    "babel-plugin-module-resolver": "4.1.0",
    "clsx": "^1.1.1",
    "cordova-plugin-network-information": "2.0.2",
    "cordova-sqlite-storage": "5.1.0",
    "formik": "^2.2.6",
    "i18next": "19.9.2",
    "ionicons": "5.5.0",
    "leaflet": "1.7.1",
    "leaflet.markercluster": "1.4.1",
    "mobx": "6.1.8",
    "mobx-react": "7.1.0",
    "po2json": "^1.0.0-beta-3",
    "prop-types": "15.7.2",
    "prop-types-exact": "^1.2.0",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-i18next": "11.8.9",
    "react-leaflet": "2.7.0",
    "react-leaflet-control": "^2.1.2",
    "react-leaflet-markercluster": "^2.0.0",
    "react-router": "5.2.0",
    "react-router-dom": "5.2.0",
    "yup": "0.32.9"
  },
  "devDependencies": {
    "@babel/core": "7.13.10",
    "@babel/plugin-proposal-class-properties": "7.13.0",
    "@babel/plugin-proposal-decorators": "7.13.5",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-transform-exponentiation-operator": "7.12.13",
    "@babel/preset-env": "7.13.10",
    "@babel/preset-react": "7.12.13",
    "@capacitor/cli": "2.4.7",
    "@flumens/eslint-config": "0.4.0",
    "@flumens/webpack-config": "0.5.0",
    "babel-eslint": "10.1.0",
    "cordova-res": "0.15.3",
    "dotenv": "8.2.0",
    "eslint": "7.21.0",
    "eslint-import-resolver-babel-module": "5.2.0",
    "eslint-plugin-import": "2.22.1",
    "husky": "5.1.3",
    "jest": "^26.6.3",
    "mobx-react-devtools": "6.1.1",
    "mock-geolocation": "1.0.11",
    "prettier-eslint": "12.0.0",
    "prettier-eslint-cli": "5.0.1",
    "regenerator-runtime": "0.13.7",
    "sharp-cli": "1.14.1",
    "vercel": "21.3.3",
    "webpack": "5.15.0",
    "webpack-cli": "4.4.0",
    "webpack-dev-server": "3.11.2"
  },
  "browserslist": [
    "Android >= 5.1",
    "iOS >= 10"
  ],
  "engines": {
    "node": ">=14",
    "npm": ">=6"
  },
  "author": {
    "name": "Flumens",
    "email": "enquiries@flumens.io",
    "url": "https://flumens.io"
  },
  "babel": {
    "presets": [
      "@babel/preset-react",
      [
        "@babel/env",
        {
          "targets": {
            "android": "5.1",
            "ios": "10.2"
          },
          "modules": "commonjs",
          "corejs": "3.0.0",
          "useBuiltIns": "usage"
        }
      ]
    ],
    "plugins": [
      [
        "module-resolver",
        {
          "root": [
            "./src"
          ],
          "alias": {
            "@apps": "./src/common/appsBitCollection",
            "common": "./src/common",
            "helpers": "./src/common/helpers",
            "savedSamples": "./src/common/models/savedSamples",
            "sample": "./src/common/models/sample",
            "appModel": "./src/common/models/appModel",
            "userModel": "./src/common/models/userModel",
            "Components": "./src/common/Components",
            "config": "./src/common/config/config.js"
          }
        }
      ],
      [
        "@babel/plugin-proposal-decorators",
        {
          "legacy": true
        }
      ],
      "@babel/plugin-transform-exponentiation-operator",
      [
        "@babel/plugin-proposal-class-properties",
        {
          "loose": true
        }
      ],
      "@babel/plugin-syntax-dynamic-import"
    ]
  },
  "eslintConfig": {
    "env": {
      "browser": true
    },
    "parser": "babel-eslint",
    "extends": "@flumens",
    "settings": {
      "import/resolver": {
        "babel-module": {}
      }
    },
    "rules": {
      "jsx-a11y/accessible-emoji": 0
    }
  },
  "prettier": {
    "printWidth": 80,
    "singleQuote": true,
    "jsxBracketSameLine": false,
    "trailingComma": "es5",
    "arrowParens": "avoid"
  },
  "bit": {
    "env": {
      "compiler": "bit.envs/compilers/react@1.0.14"
    },
    "componentsDefaultDirectory": "components/{name}",
    "packageManager": "npm"
  }
}
